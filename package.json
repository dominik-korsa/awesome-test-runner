{
  "name": "test-jail",
  "version": "2.2.2",
  "description": "Safely test console programs in Docker containers",
  "keywords": [
    "cli",
    "testing",
    "runner",
    "typescript",
    "cpp",
    "python",
    "docker",
    "jail",
    "vm"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "test-jail": "dist/bin/index.js",
    "testjail": "dist/bin/index.js"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc -w",
    "test": "mocha -r ts-node/register tests/**.ts",
    "test:cov": "nyc --reporter=lcov npm run test",
    "prepublishOnly": "npm run build",
    "release": "release-it --only-version",
    "lint": "eslint src tests -c .eslintrc.json",
    "lint:fix": "eslint src tests -c .eslintrc.json --fix",
    "docs": "typedoc"
  },
  "author": "Dominik Korsa <dominik.korsa@gmail.com>",
  "repository": "https://github.com/dominik-korsa/test-jail",
  "license": "MIT",
  "dependencies": {
    "@types/diff": "^4.0.2",
    "@types/dockerode": "^3.2.1",
    "@types/fs-extra": "^9.0.3",
    "@types/glob": "^7.1.3",
    "@types/lodash": "^4.14.165",
    "@types/node": "^14.14.6",
    "@types/replace-ext": "^2.0.0",
    "@types/tar-stream": "^2.1.0",
    "@types/update-notifier": "^5.0.0",
    "@types/yargs": "^15.0.9",
    "ansi-escapes": "^4.3.1",
    "chalk": "^4.1.0",
    "cli-table3": "^0.6.0",
    "diff": "^5.0.0",
    "dockerode": "^3.2.1",
    "enquirer": "^2.3.6",
    "eol": "^0.9.1",
    "fs-extra": "^9.0.1",
    "glob": "^7.1.6",
    "lodash": "^4.17.20",
    "open": "^7.3.0",
    "ora": "^5.1.0",
    "read-pkg-up": "^7.0.1",
    "replace-ext": "^2.0.0",
    "slash": "^3.0.0",
    "tar-stream": "^2.1.4",
    "update-notifier": "^5.0.1",
    "yargs": "^16.1.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/chai-as-promised": "^7.1.3",
    "@types/mocha": "^8.0.3",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^7.12.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-plugin-import": "^2.22.1",
    "esm": "^3.2.25",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "release-it": "^14.2.1",
    "ts-node": "^9.0.0",
    "typedoc": "^0.19.2",
    "typedoc-plugin-jekyll": "^0.1.0",
    "typescript": "^4.0.5"
  }
}
